{
    "name": "govCMS/govCMS",
    "description": "The govCMS7 build.",
    "require-dev": {
        "behat/behat": "^3.4",
        "drupal/coder": "^8.2",
        "drupal/drupal-extension": "^3.4",
        "drush/drush": "^8.1",
        "fabpot/goutte": "^3.2",
        "jakoch/phantomjs-installer": "^2.1",
        "phing/phing": "^2.16",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^5.7",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "^2.9"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "secure-http": false,
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
