{
    "name": "govCMS/govCMS",
    "description": "The govCMS7 build.",
    "require-dev": {
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "dmore/behat-chrome-extension": "^1.2",
        "drupal/coder": "^8.2",
        "drupal/drupal-extension": "^4.0",
        "drush/drush": "~8",
        "fabpot/goutte": "^3.2",
        "grasmash/drupal-security-warning": "^1.1",
        "jakoch/phantomjs-installer": "1.9.8",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.3",
        "phing/phing": "^2.16",
        "phploc/phploc": "^3.0 || ^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^4.8.35 || ^6.1",
        "se/selenium-server-standalone": "^3.12",
        "sebastian/phpcpd": "^3.0"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "secure-http": false,
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "conflict": {
       "drupal/core": "8.*",
       "drush/drush": ">9"
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "suggest": {
        "hirak/prestissimo": "^0.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
